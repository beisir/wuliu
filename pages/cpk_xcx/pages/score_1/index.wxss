.desNum{float:right;width:60%;color:#ef322c;}
.d_1{width:30%;display:inline-block;}
.d_2{color:#333;}

/* 底部 */
.w_bot{width:100%;height:96rpx;line-height:96rpx;font-size:30rpx;color:#333;background:#fff;position:fixed;left:0;bottom:0;box-shadow: 0 -4rpx 6rpx #ddd;text-align:center;display:block;}
.b_icon{width:36rpx;height:36rpx;display:inline-block;background:url(https://style.org.hc360.com/images/cpk/p_c_ico2.png) no-repeat -154rpx 0;background-size:194rpx 36rpx; vertical-align: middle;margin-right:15rpx;}