.scoreBox{clear:both;background:#fff;padding:10rpx 50rpx;line-height:75rpx;overflow:hidden;}
.together{font-size:34rpx;color:#333;font-weight:600;}
.scoreTil{float:left;width:40%;}
.scoreNum{float:right;width:60%;color:#ef322c;}
.desCon{clear:both;font-size:30rpx;color:#333;}
