.filtrate {
  line-height: 60rpx;
  display: flex;
  justify-content: space-around;
  background-color: #fff;
  border-bottom:1px #dcdcdc solid;
}

.logistic-group {
  display: flex;
  justify-content: space-betweenl;
  flex-wrap: wrap;
}

.logistic-checkbox {
  width: 50%;
  font-size: 26rpx;
  margin: 10rpx 0;
}

.fillate-wrap {
  width: 100%;
  height: 100%;
  background: rgba(255, 255, 255, 0);
  position: fixed;
  top: 0;
  right: -100%;
  bottom: 0;
  z-index: 10000;
  display: flex;
  flex-direction: row-reverse;
  transition: all 0.5s ease-in-out;
}

.fillate-content {
  width: 80%;
  height: 100%;
  background: white;
  position: relative;
}

.fillate-title {
  color: #999;
  font-size: 30rpx;
}

.fillate-items {
  display: flex;
  margin-top: 20rpx;
  flex-wrap: wrap;
}

.fillate-item {
  color: #666;
  font-size: 24rpx;
  padding: 10rpx 24rpx;
  background: #eee;
  border-radius: 40rpx;
  margin:20rpx 10rpx;
}

.active {
  color: #fff;
  font-size: 24rpx;
  padding: 10rpx 24rpx;
  background: #e99191;
  border-radius: 40rpx;
  margin:20rpx 10rpx;
}

.fillate-button {
  position: absolute;
  bottom: 0;
  width: 100%;
}

.fillate-button-btn {
  width: calc(50% - 2px);
  text-align: center;
  display: inline-block;
  padding: 20rpx 0;
  border: 1px solid #eee;
  font-size: 34rpx;
  color: rgb(90, 90, 90);
}

.confirm {
  background: #fd473c;
  color: white;
}

.search-head {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  z-index: 100;
  background: #eee;
}

