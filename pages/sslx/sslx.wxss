/* pages/favorites/favorites.wxss */

page {
  background: #f7f7f7;
}

.searchWrap {
  background: #fff;
  height: 80rpx;
  padding: 20rpx;
}

.searchInput {
  position: relative;
  height: 80rpx;
}

.searchInput view {
  background: #eee;
  font-size: 28rpx;
  height: 66rpx;
  border-radius: 33rpx;
  line-height: 66rpx;
  border: none;
  text-align: center;
  color: #666;
}

.searchInput:before {
  content: "";
  display: block;
  position: absolute;
  top: 4rpx;
  left: 20rpx;
  height: 60rpx;
  width: 60rpx;
  background: url(https://style.org.hc360.com/images/logistics/s-program/wlIco.png) 10rpx 17rpx no-repeat;
  background-size: 300rpx 600rpx;
}

.searchList {
  margin: 20rpx 20rpx 0;
  background: #fff;
  padding: 0 20rpx;
  overflow: hidden;
}

.listTop {
  font-size: 28rpx;
  color: #333;
  padding: 5rpx 0;
  display: flex;
  align-items: center;
}

.listTop .comName {
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}

.listTop .kmNum {
  float: right;
  display: block;
  width: 52rpx;
  height: 52rpx;
  margin-right: 6rpx;
  background: url(https://style.org.hc360.com/images/logistics/s-program/wlIco2.png) no-repeat 0 -400rpx;
  background-size: 300rpx 600rpx;
  margin-top: 20rpx;
  cursor: pointer;
}

